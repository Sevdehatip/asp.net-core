@using CoreDepartmentApp.Models
@model List<employee>
@{
    ViewData["Title"] = "DepartDetails";
    Layout = "~/Views/Shared/_TestLayout.cshtml";
}

<br />
<h2>@ViewBag.departname Departmanı</h2>
<br />
<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>Ad</th>
        <th>Soyad</th>
        <th>Şehir</th>
    </tr>
    @foreach (var x in Model)
    {
        <tr>
            <td>@x.id</td>
            <td>@x.name</td>
            <td>@x.lastname</td>
            <td>@x.city</td>
        </tr>
    }

</table>

